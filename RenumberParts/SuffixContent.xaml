<Window x:Class="RenumberParts.SuffixContent"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:sw="clr-namespace:WpfStyleableWindow.StyleableWindow"
    Title="Warning" Height="143" Width="244" Style="{DynamicResource Warning}" MaxHeight="143" MaxWidth="244" MinHeight="143" MinWidth="244">
    <Grid Margin="0,0,0,0" Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Button Style="{DynamicResource SecondaryButtonStyle}" Margin="81,69,81,0" x:Name="Button1" IsCancel="True" Content="Accept" Click="Button1_Click" Height="23" VerticalAlignment="Top" Grid.Column="1"/>
        <Label Style="{DynamicResource LabelsTitle}" HorizontalContentAlignment="Right" Content="Number field should contain only &#xD;&#xA;numbers" HorizontalAlignment="Center" Margin="22,16,17,0" VerticalAlignment="Top" Grid.Column="1" Height="42"/>
    </Grid>

    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
</Window>
